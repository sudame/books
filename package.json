{
  "name": "books",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "run-p -l watch:*",
    "watch:vite": "vite",
    "watch:tcm": "tcm src -w -c",
